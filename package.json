{
  "name": "geomaniac",
  "version": "0.5.1",
  "private": true,
  "type": "module",
  "license": "CC BY-NC-SA 4.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "netlify dev",
    "dev:watch": "conc 'pnpm:watch:*'",
    "watch:vite": "vite",
    "watch:css": "pnpm generate:css --watch",
    "build": "netlify build",
    "build:start": "pnpm build:css && pnpm build:prod",
    "build:css": "pnpm generate:css --minify",
    "build:prod": "vite build",
    "generate:css": "tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css",
    "preview": "vite preview",
    "serve": "netlify serve",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc",
    "validate": "pnpm lint && pnpm typecheck",
    "dead-code": "ts-prune | grep -v netlify"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.5.0",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-devtools": "^5.64.2",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "motion": "^12.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-leaflet": "^5.0.0",
    "react-redux": "^9.2.0",
    "react-responsive-masonry": "^2.7.0",
    "react-router": "^7.1.3",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@netlify/edge-functions": "^2.11.1",
    "@netlify/functions": "^3.0.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/eslint-plugin-query": "^5.64.2",
    "@types/gapi": "^0.0.47",
    "@types/geojson": "^7946.0.15",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/react-responsive-masonry": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^15.14.0",
    "markdownlint": "^0.37.4",
    "netlify-cli": "^18.0.1",
