{
  "name": "geomaniac",
  "version": "0.5.1",
  "private": true,
  "type": "module",
  "license": "CC BY-NC-SA 4.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "netlify dev",
    "dev:watch": "conc 'pnpm:watch:*'",
    "watch:vite": "vite",
    "watch:css": "pnpm generate:css --watch",
    "build": "netlify build",
    "build:start": "pnpm build:css && pnpm build:prod",
    "build:css": "pnpm generate:css --minify",
    "build:prod": "vite build",
    "generate:css": "tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css",
    "preview": "vite preview",
    "serve": "netlify serve",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc",
    "validate": "pnpm lint && pnpm typecheck",
    "dead-code": "ts-prune | grep -v netlify"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@react-spring/web": "^9.7.5",
    "@tanstack/react-query": "^5.60.2",
    "@tanstack/react-query-devtools": "^5.60.2",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-leaflet": "^4.2.1",
    "react-responsive-masonry": "^2.4.1",
    "react-router-dom": "^6.28.0",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@netlify/edge-functions": "^2.11.0",
    "@netlify/functions": "^2.8.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/eslint-plugin-query": "^5.60.1",
    "@types/gapi": "^0.0.47",
    "@types/geojson": "^7946.0.14",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-responsive-masonry": "^2.1.3",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "globals": "^15.12.0",
    "markdownlint": "^0.36.1",
    "netlify-cli": "^17.37.2",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwind-scrollbar": "^3.1.0",
