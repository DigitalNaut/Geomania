{
  "name": "geomaniac",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "conc 'pnpm:dev-*'",
    "dev-run": "vite",
    "dev-css": "pnpm generate:css --watch",
    "build": "pnpm build-css && pnpm build-prod",
    "build-css": "pnpm generate:css --minify",
    "build-prod": "tsc && vite build",
    "generate:css": "tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "validate": "pnpm lint && pnpm typecheck"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-oauth/google": "^0.8.0",
    "@react-spring/web": "^9.7.1",
    "@tanstack/react-query": "^4.26.1",
    "axios": "^1.3.4",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.9.0",
    "realm-web": "^2.0.0"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/eslint-plugin-query": "^4.26.2",
    "@types/gapi": "^0.0.44",
    "@types/gapi.client.drive": "^3.0.15",
    "@types/geojson": "^7946.0.10",
    "@types/google.accounts": "^0.0.7",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "prettier": "^2.8.4",
    "tailwind-scrollbar": "^3.0.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2",
    "vite": "^4.2.0",
    "vite-plugin-svgr": "^2.4.0"
  }
}
