{
  "name": "geomaniac",
  "version": "0.5.1",
  "private": true,
  "type": "module",
  "license": "CC BY-NC-SA 4.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "netlify dev",
    "dev:watch": "conc 'pnpm:watch:*'",
    "watch:vite": "vite",
    "watch:css": "pnpm generate:css --watch",
    "build": "netlify build",
    "build:start": "pnpm build:css && pnpm build:prod",
    "build:css": "pnpm generate:css --minify",
    "build:prod": "vite build",
    "generate:css": "tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css",
    "preview": "vite preview",
    "serve": "netlify serve",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc",
    "validate": "pnpm lint && pnpm typecheck",
    "dead-code": "ts-prune | grep -v netlify"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@react-oauth/google": "^0.12.1",
    "@react-spring/web": "^9.7.4",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.26.2",
    "tailwind-merge": "^2.5.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@netlify/edge-functions": "^2.11.0",
    "@netlify/functions": "^2.8.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/eslint-plugin-query": "^5.58.1",
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.drive-v3": "^0.0.4",
    "@types/geojson": "^7946.0.14",
    "@types/google.accounts": "^0.0.14",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.10.0",
    "markdownlint": "^0.34.0",
