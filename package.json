{
  "name": "geomaniac",
  "version": "0.4.1",
  "private": true,
  "type": "module",
  "license": "CC BY-NC-SA 4.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "netlify dev",
    "dev:watch": "conc 'pnpm:watch:*'",
    "watch:vite": "vite",
    "watch:css": "pnpm generate:css --watch",
    "build": "netlify build",
    "build:start": "pnpm build:css && pnpm build:prod",
    "build:css": "pnpm generate:css --minify",
    "build:prod": "vite build",
    "generate:css": "tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css",
    "preview": "vite preview",
    "serve": "netlify serve",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "validate": "pnpm lint && pnpm typecheck",
    "dead-code": "ts-prune | grep -v netlify"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-oauth/google": "^0.11.1",
    "@react-spring/web": "^9.7.3",
    "@tanstack/react-query": "^4.32.6",
    "@tanstack/react-query-devtools": "^4.32.6",
    "axios": "^1.4.0",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.14.2",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/eslint-plugin-query": "^4.32.5",
    "@types/gapi": "^0.0.44",
    "@types/gapi.client.drive-v3": "^0.0.1",
    "@types/geojson": "^7946.0.10",
    "@types/google.accounts": "^0.0.8",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^20.4.8",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitejs/plugin-react": "^4.0.4",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "markdownlint": "^0.29.0",
    "netlify-cli": "^15.10.0",
    "prettier": "^3.0.1",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.3.3",
    "ts-prune": "^0.10.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-svgr": "^3.2.0"
  }
}
