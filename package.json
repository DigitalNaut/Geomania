{
  "name": "geomania",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "conc 'pnpm:dev-*'",
    "dev-run": "vite",
    "dev-css": "pnpm generate:css --watch",
    "build": "pnpm build-css && pnpm build-prod",
    "build-css": "pnpm generate:css --minify",
    "build-prod": "tsc && vite build",
    "generate:css": "tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "pnpm exec tsc -b"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-oauth/google": "^0.6.1",
    "axios": "^1.3.2",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-leaflet": "^4.2.0",
    "realm-web": "^2.0.0"
  },
  "devDependencies": {
    "@types/gapi": "^0.0.44",
    "@types/gapi.client.drive": "^3.0.14",
    "@types/geojson": "^7946.0.10",
    "@types/google.accounts": "^0.0.5",
    "@types/leaflet": "^1.9.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.0.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "prettier": "^2.8.3",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vite-plugin-svgr": "^2.4.0"
  }
}
