{
  "name": "geomaniac-tools",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "wiki-data": "bun src/wiki-data/index.ts",
    "svg-map": "bun --env-file=src/svg-map/.env src/svg-map/index.ts",
    "country-store": "bun src/country-store/index.ts",
    "clean": "run-p clean:*",
    "clean:svg-map": "rm -rf src/svg-map/out/",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier . -w",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "geomaniac",
    "tools"
  ],
  "author": "DigitalNaut",
  "license": "CC BY-NC-SA 4.0",
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.5",
    "bun": "^1.1.43",
    "eslint": "^9.18.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "mapshaper": "^0.6.102",
    "vitest": "^2.1.8"
  }
}
