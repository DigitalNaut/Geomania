  "dotenv.enableAutocloaking": false,
  "deno.enable": true,
  "deno.lint": true,
  "deno.enablePaths": [
    "netlify/edge-functions"
  ],
  "deno.unstable": true,
  "deno.importMap": ".netlify/edge-functions-import-map.json",
  "markdownlint.config": {
    "MD033": {
      "allowed_elements": [
        "details",
        "summary"
      ]
    }
  },
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    "class:list",
    ".+Styles?"
  ],
  "tailwindCSS.experimental.classRegex": [
    ".*Styles?",
    ["Classes \\=([^;]*);", "'([^']*)'"],
    ["Classes \\=([^;]*);", "\"([^\"]*)\""],
    ["Classes \\=([^;]*);", "\\`([^\\`]*)\\`"]
  ]
}